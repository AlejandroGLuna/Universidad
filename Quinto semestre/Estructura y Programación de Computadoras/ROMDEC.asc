      ORG $8000


INICIO
      CLR ORDEN
      CLR CONT_IMPRIME_ERR
      CLR VAR
      CLR U_MILLAR
      CLR U_CENTENA
      CLR U_DECENA
      CLR U_UNIDAD
      CLR CONTADOR_MILLARES	
      CLR CONTADOR_CENTENAS	
      CLR CONTADOR_DECENAS	
      CLR CONTADOR_UNIDADES
      LDS #$00FF
      JSR SERIAL
      CLR NUM_LETRAS_LEIDAS
      CLR CONT_IMPRIME_ERR	
      CLR ESCRIBE_CARACT		
      CLR DIR_LIMPIA
      CLR CATEGORIA

		*Espera hasta que se reciba un carácter diferente de ‘?’
CICLATE
      LDAA #'?
      STAA ORDEN


CICLO
      LDAB ORDEN
      CMPB #'?
      BEQ  CICLO

      LDX #DIR_BASE
      CLR VAR
      CLR CONTADOR_OK
      STAB $00,X
      INX

	* Para números decimales el guardado empieza en $0050 como mraca en las instrucciones
LOOP
      LDAA VAR
      BEQ  LOOP
      CLR  VAR
      

      LDAB ORDEN	*El inicio de la cadena se almacena en ORDEN
      STAB $00,X
      
      INX
      INC NUM_LETRAS_LEIDAS	*Número de letras leídas, cada que se ingresa se incrementa 

       *****************Se verifica que ORDEN tenga el carácter O y luego K***************

      CMPB #$3D 		* signo =
      BEQ EXITOS
      

     *Si no existe una O o una K regresa a realizar la escritura*

      BNE LOOP


****************Validar OK*******************
*Si se tiene OK se salta a dirección base

EXITOS
      LDX #DIR_BASE

****Se compara si lo que se recibe es M, D, C, L, X, V o I; si lo es válida que se introduce numero en romano****

*El primer valor lo pone en la $0050

      LDAA $00,X      
      *revisa si los caracteres son romanos
      CMPA #'M
      BEQ REVISA_ES_ROMANO		
      CMPA #'D
      BEQ REVISA_ES_ROMANO	
      CMPA #'C
      BEQ REVISA_ES_ROMANO	
      CMPA #'L
      BEQ REVISA_ES_ROMANO	
      CMPA #'X 
      BEQ REVISA_ES_ROMANO	
      CMPA #'V
      BEQ REVISA_ES_ROMANO
      CMPA #'I 
      BEQ REVISA_ES_ROMANO

      *Si no se tiene M, D, C, L, X, V o I entonce es decimal
      
      LDX #DIR_BASE
      LDAA #$39
      JMP CHECK_DECIMAL			*Valida que lo que se reciba sea formato decimal


*Revisa si todos los otros valores ingrsados son letras romanas

CHECK_ROMANO
      LDAA $00,X      
      				*revisa si los caracteres son romanos
      CMPA #'M
      BEQ REVISA_ES_ROMANO		
      CMPA #'D
      BEQ REVISA_ES_ROMANO	
      CMPA #'C
      BEQ REVISA_ES_ROMANO	
      CMPA #'L
      BEQ REVISA_ES_ROMANO	
      CMPA #'X 
      BEQ REVISA_ES_ROMANO	
      CMPA #'V
      BEQ REVISA_ES_ROMANO
      CMPA #'I 
      BEQ REVISA_ES_ROMANO		*Salta si 0
      
      BNE VE_ERR_ROMANO		*Si no es algun caracter en romano, entonces se va a error

VE_ERR_ROMANO
      JMP	ERR_ROMANO	*Mensaje de error



*Revisa si el siguiente dato es igual, si es as� manda a exito romano, caso contrario sigue revisando

REVISA_ES_ROMANO
      INX
      LDAA $00,X
      CMPA #$3D			*=
      BEQ EXITO_ES_ROMANO
      BNE CHECK_ROMANO	 *Si hay un caracter invalido, salta a comparar los caracteres romanos



EXITO_ES_ROMANO
	LDAA	#'R
	STAA	CATEGORIA			 *CARGA UNA R EN EL Acc A (identificador)

	LDAA	NUM_LETRAS_LEIDAS		*Número de letras leídas
	STAA	POS_ACTUAL			*dígito actual que está leyendo

	DEC	POS_ACTUAL
	INCA
	ADDA	#DIR_BASE
	STAA	DIR_MENSAJE		*Dirección de mensaje a mostrar 0050
	LDAA	#$50
	ADDA	POS_ACTUAL
	ADDA	#$02
	STAA	DIR_LIMPIA		*Dirección de memoria a limpiar
	JMP	CLEAR_DIRMEM		*Se limpia la sección de memoria empleada
	
ROMANO
	CLR	CONTADOR_1
	CLR	CONTADOR_2
	CLR	IS_PRIN
	CLR	IS_T
	CLR	IS_C
	CLR	IS_L
	CLR	IS_D
	CLR	IS_M

					*Si hay I
CHECA_I				

	LDX	#DIR_BASE
	XGDX
	ADDB	POS_ACTUAL
	XGDX
	LDAA	$00,X
	STAA	CAR_LEIDO		*Carácter que se está leyendo
	DEC	POS_ACTUAL		
	CMPA	#'I			*El que se está leyendo es I?
	BEQ	ESI			*Si entonces es I, ir a etiqueta
	CMPA	#'V			*Entonces si no es I es V?
	BEQ	ESV			*saltar a es V
	JMP	CHECA_X_2


CHECA_X
	LDX	#DIR_BASE
	XGDX
	ADDB	POS_ACTUAL
	XGDX
	LDAA	$00,X
	STAA	CAR_LEIDO
	DEC	POS_ACTUAL


CHECA_X_2
	CMPA	#'X		*CAR_LEIDO dígito actual es X sino fue I o V
	BEQ	ESX		*salta a X
	CMPA	#'L		 *compara con L
	BNE	CHECA_C_2
	JMP	ESL


CHECA_C
	LDX	#DIR_BASE
	XGDX
	ADDB	POS_ACTUAL
	XGDX
	LDAA	$00,X
	STAA	CAR_LEIDO
	DEC	POS_ACTUAL
CHECA_C_2
	CMPA	#'C
	BEQ	VE_ES_C
	CMPA	#'D
	BNE	CHECA_M
	JMP	ESD

VE_ES_C
	JMP	ESC

CHECA_M
	CMPA	#'M
	BEQ	VE_ES_M
	CMPA	#'&
	BEQ	VE_FINISH
	JMP	ERR_ROMANO


VE_ES_M
	JMP	ESM

VE_FINISH
	JMP	FINISH

*Realiza las comparaciones necesarias, primero I, V, luego X… cuando válida si son iguales salta a etq Es(I,X,C,L,M)

						*Para verificar que únicamente lleguen 3 III
ESI				
	LDAA	IS_PRIN
	CMPA	#$03
	BLS	CORRECTOI
	JMP	ERR_ROMANO


CORRECTOI
	INC	CONTADOR_2
	INC	IS_PRIN
	JMP	CHECA_I
					*Verifica que lleguen únicamente a lo mucho VVV
ESV			
	LDAA	IS_PRIN
	CMPA	#$04
	BLS	CORRECTOV
	JMP	ERR_ROMANO


CORRECTOV
	LDAA	CONTADOR_2
	ADDA	#$05			*Suma 5
	STAA	CONTADOR_2
	INC	IS_PRIN
	LDX	#DIR_BASE
	XGDX
	ADDB	POS_ACTUAL
	XGDX
	LDAA	$00,X
	STAA	CAR_LEIDO
	DEC	POS_ACTUAL
	CMPA	#'I			*Si después de la V sigue I
	BNE	CHECA_X_2
	LDAA	IS_PRIN
	CMPA	#$01
	BNE	VE_ERROR
	DEC	CONTADOR_2
	INC	IS_PRIN
	JMP	CHECA_X


VE_ERROR
	JMP	ERR_ROMANO


ESX
	LDAA	IS_T
	CMPA	#$03
	BLS	CORRECTOX
	JMP	ERR_ROMANO


CORRECTOX
	LDAA	CONTADOR_2
	ADDA	#$0A			*suma 10 (A0 en hexadecimal)
	STAA	CONTADOR_2
	INC	IS_PRIN
	INC	IS_T
	LDX	#DIR_BASE
	XGDX
	ADDB	POS_ACTUAL
	XGDX
	LDAA	$00,X
	STAA	CAR_LEIDO
	DEC	POS_ACTUAL
	CMPA	#'I
	BNE	SALTACHECA_X_2
	LDAA	IS_T
	CMPA	#$01
	BNE	VE_ERROR
	DEC	CONTADOR_2
	INC	IS_PRIN
	INC	IS_T
	JMP	CHECA_X


SALTACHECA_X_2
	JMP	CHECA_X_2


ESL
	LDD	CONTADOR_1
	ADDD	#$32		*Sumamos 32 hexadecimal al valor que ya tenía

	STD	CONTADOR_1
	INC	IS_PRIN
	INC	IS_L
	LDX	#DIR_BASE
	XGDX
	ADDB	POS_ACTUAL
	XGDX
	LDAA	$00,X
	STAA	CAR_LEIDO
	DEC	POS_ACTUAL
	CMPA	#'X
	BNE	SALTACHECA_C_2
	LDAA	IS_L
	CMPA	#$01
	BNE	VE_ERROR ##CHECAR LINEA
	LDAA	IS_T
	CMPA	#$00
	BNE	VE_ERROR
	LDAA	CONTADOR_2
	SUBA	#$0A
	STAA	CONTADOR_2
	INC	IS_PRIN
        JMP     CHECA_C #MODIFACIÓN 


SALTACHECA_C_2
	JMP	CHECA_C_2


ESC
	LDAA	IS_C
	CMPA	#$03
	BLS	CORRECTOC
	JMP	ERR_ROMANO


CORRECTOC
	LDD	CONTADOR_1
	ADDD	#$0064		* Se le suma 100 (64 en hexadecimal) al valor numerico
	STD	CONTADOR_1
	INC	IS_PRIN
	INC	IS_C
	LDX	#DIR_BASE
	XGDX
	ADDB	POS_ACTUAL
	XGDX
	LDAA	$00,X
	STAA	CAR_LEIDO
	DEC	POS_ACTUAL
	CMPA	#'X
	BNE	SALTACHECA_C_2
	LDAA	IS_C
	CMPA	#$01
	BNE	VE_ERROR2
	LDD	CONTADOR_1
	SUBD	#$0A
	STD	CONTADOR_1
	INC	IS_PRIN
	INC	IS_C
	JMP	CHECA_C


VE_ERROR2
	JMP	ERR_ROMANO

ESD
	LDD	CONTADOR_1
	ADDD	#$01F4		* Se le suma 500 (01F4 en hexadecimal) al valor numerico
	STD	CONTADOR_1
	INC	IS_PRIN
	INC	IS_D
	LDX	#DIR_BASE
	XGDX
	ADDB	POS_ACTUAL
	XGDX
	LDAA	$00,X
	STAA	CAR_LEIDO
	DEC	POS_ACTUAL
	CMPA	#'C
	BNE	SALTACHECA_M
	LDAA	IS_C
	CMPA	#$00
	BNE	VE_ERROR2
	LDAA	IS_D
	CMPA	#$01
	BNE	VE_ERROR2
	LDD	CONTADOR_1
	SUBD	#$0064
	STD	CONTADOR_1
	INC	IS_PRIN
	INC	IS_C

        LDX	#DIR_BASE
	XGDX
	ADDB	POS_ACTUAL
	XGDX
	LDAA	$00,X
	STAA	CAR_LEIDO
	DEC	POS_ACTUAL


SALTACHECA_M
	JMP	CHECA_M

ESM
	LDD	CONTADOR_1
	ADDD	#$03E8		* Se le suma 1000 (03E8 en hexadecimal) al valor numerico
	STD	CONTADOR_1
	INC	IS_PRIN
	INC	IS_M
	LDX	#DIR_BASE
	XGDX
	ADDB	POS_ACTUAL
	XGDX
	LDAA	$00,X
	STAA	CAR_LEIDO
	DEC	POS_ACTUAL
	CMPA	#'C
	BNE	IS_NOT_C
	LDAA	IS_M
	CMPA	#$01
	BNE	ERR_ROMANO
	LDD	CONTADOR_1
	SUBD	#$0064
	STD	CONTADOR_1
	INC	IS_PRIN
	INC	IS_M
	LDX	#DIR_BASE
	XGDX
	ADDB	POS_ACTUAL
	XGDX
	LDAA	$00,X
	DEC	POS_ACTUAL
	JMP	CHECA_M
IS_NOT_C
	LDAA	IS_PRIN
	CMPA	#$14		* Revisa si ya no faltan letras por comparar
	BHS	FINISH
	LDAA	$00,X
	JMP	CHECA_M


FINISH

*Una vez validado que lo que se recibe es un número romano y tiene el formato correcto, se hacen las sumas correspondientes por 1, 5, 10, 100, para hacer la conversión a decimal


*****	Conversion a decimal*****

	LDD	CONTADOR_1
	LDX	#$03E8
	IDIV
	XGDX
	STAB	U_MILLAR
	XGDX
	LDX	#$0064
	IDIV
	XGDX
	STAB	U_CENTENA
	XGDX
	LDX	#$000A
	IDIV
	XGDX
	STAB	U_DECENA
	XGDX
	STAB	U_UNIDAD
*****	IMPRIME EL NUMERO DECIMAL	*****
	LDAA	#$00
	LDAB	DIR_MENSAJE
	XGDX

PRINT_M
	LDAA	U_MILLAR		* unidades de millar
	ADDA	#$30
	CMPA	#$00
	BEQ	PRINT_C
	STAA	$00,X
PRINT_C
	LDAA	U_CENTENA		*unidades de centenas
	ADDA	#$30
	CMPA	#$00
	BEQ	PRINT_D
	STAA	$01,X
PRINT_D
	LDAA	U_DECENA		*unidades de decenas
	ADDA	#$30
	CMPA	#$00
	BEQ	PRINT_U
	STAA	$02,X
PRINT_U
	LDAA	U_UNIDAD		*unidades de unidades
	ADDA	#$30
	STAA	$03,X
	JMP	WAIT


ERR_ROMANO
	LDX	#DIR_ERROR
	XGDX
	ADDB	CONT_IMPRIME_ERR
	XGDX
      	LDAA	$00,X
	CMPA	#'$
	BEQ	SALTA2WAIT
	STAA	ESCRIBE_CARACT	
	LDD	#DIR_BASE
	ADDB	NUM_LETRAS_LEIDAS
	ADDB	CONT_IMPRIME_ERR
	INCB
	XGDX
	LDAA	ESCRIBE_CARACT	
	STAA	$00,X
	INC	CONT_IMPRIME_ERR
      	JMP ERR_ROMANO

SALTA2WAIT
        
	JMP	WAIT


**********************************************************
**********************************************************
CHECK_DECIMAL
      LDAB $00,X
      CBA 
      BEQ REVISA_ES_DECIMAL
      DECA
      CMPA #$00
      BNE CHECK_DECIMAL
      BEQ VE_ERROR_DECIMAL

VE_ERROR_DECIMAL
	JMP	ERRORDECIMAL

REVISA_ES_DECIMAL
      LDAA #$39
      INX
      LDAB $00,X
      CMPB #$3D
      BEQ REVISA_ES_DECIMAL_RANGO
      BNE CHECK_DECIMAL


REVISA_ES_DECIMAL_RANGO
      
      LDAA NUM_LETRAS_LEIDAS
      CMPA #01
      BEQ EXITO_ES_DECIMAL
      CMPA #02
      BEQ EXITO_ES_DECIMAL
      CMPA #03
      BEQ EXITO_ES_DECIMAL
      CMPA #04
      BEQ EXITO_ES_DECIMAL
      BNE VE_ERROR_DECIMAL

EXITO_ES_DECIMAL
	LDAA	NUM_LETRAS_LEIDAS
	STAA	POS_ACTUAL
	DEC	POS_ACTUAL
	INCA
	ADDA	#DIR_BASE
	STAA	DIR_MENSAJE

	LDAA	#$50
	ADDA	POS_ACTUAL
	ADDA	#$02
	STAA	DIR_LIMPIA
	JMP	CLEAR_DIRMEM

SALTAERRORCERO
	JMP	ERRORDECIMAL

DECIMAL
	CLR	POS_ACTUAL
	CLR	CONTADOR_1
	CLR	CONTADOR_MILLARES
	CLR	CONTADOR_CENTENAS
	CLR	CONTADOR_DECENAS
	CLR	CONTADOR_UNIDADES

OBTIENE_CARACTER
	LDX	#DIR_BASE
	XGDX
	ADDB	POS_ACTUAL
	XGDX
	LDAA	$00,X
	CMPA	#'=
	BEQ	SALTAWAIT
	
	SUBA	#$30
	STAA	CAR_LEIDO
	LDAA	NUM_LETRAS_LEIDAS
	CMPA	#$04
	BEQ	SALTAMILLARES
	LDAA	CONTADOR_1
	CMPA	#$00
	BNE	CHECK_3
	INC	CONTADOR_1
        
CHECK_3
	LDAA	NUM_LETRAS_LEIDAS
	CMPA	#$03
	BEQ	SALTACENTENAS
	LDAA	CONTADOR_1
	CMPA	#$01
	BNE	CHECK_2
	INC	CONTADOR_1


CHECK_2
	LDAA	NUM_LETRAS_LEIDAS
	CMPA	#$02
	BEQ	DECENAS
	JMP	VE_ES_UNIDAD


SALTAWAIT
	JMP	WAIT

SALTAMILLARES
	JMP	MILLARES

SALTACENTENAS
	JMP	CENTENAS

MILLARES
	LDAA	CONTADOR_1
	CMPA	#$00
	BEQ	VE_ES_MILLAR
	CMPA	#$01
	BEQ	VE_ES_CENTENA
	CMPA	#$02
	BEQ	VE_ES_DECENA
	JMP	VE_ES_UNIDAD

VE_ES_MILLAR
	JMP	ES_MILLAR

VE_ES_CENTENA
	JMP	ES_CENTENA

VE_ES_DECENA
	JMP	ES_DECENA

VE_ES_UNIDAD
	JMP	ES_UNIDAD

CENTENAS
	LDAA	CONTADOR_1
	CMPA	#$01
	BEQ	VE_ES_CENTENA
	CMPA	#$02
	BEQ	VE_ES_DECENA
	JMP	VE_ES_UNIDAD

DECENAS
	LDAA	CONTADOR_1
	CMPA	#$02
	BEQ	VE_ES_DECENA
	JMP	VE_ES_UNIDAD

ES_MILLAR
	LDAA	CAR_LEIDO
	STAA	U_MILLAR
	LDAA	CONTADOR_MILLARES
	CMPA	CAR_LEIDO
	BLO	MILLAR_1

FINMILLAR
	INC	CONTADOR_1
	INC	POS_ACTUAL
	JMP	OBTIENE_CARACTER

MILLAR_1
	LDAA	#$00
	LDAB	DIR_MENSAJE
	XGDX
	LDAA	#'M
	STAA	$00,X
	INC	DIR_MENSAJE
	INC	CONTADOR_MILLARES
	LDAA	CONTADOR_MILLARES
	CMPA	CAR_LEIDO
	BLO	MILLAR_1
	JMP	FINMILLAR

ES_CENTENA
	LDAA	CAR_LEIDO
	STAA	U_CENTENA
        CMPA    #$00
        BEQ     FINCENTENA
	CMPA	#$05
	BLO	CENTENA_MENOR_5
	CMPA	#$09
	BNE	CENTENA_NOT_9
	LDAA	#$00
	LDAB	DIR_MENSAJE
	XGDX
	LDAA	#'C
	STAA	$00,X
	INC	DIR_MENSAJE
	LDAA	#$00
	LDAB	DIR_MENSAJE
	XGDX
	LDAA	#'M
	STAA	$00,X
	INC	DIR_MENSAJE
	JMP	FINCENTENA

CENTENA_MENOR_5
	CMPA	#$04
	BEQ	CENTENA_ES_4
	LDAA	CONTADOR_CENTENAS
	CMPA	CAR_LEIDO
	BLO	CENTENA_1

CENTENA_ES_4
	LDAA	#$00                    
	LDAB	DIR_MENSAJE
	XGDX
